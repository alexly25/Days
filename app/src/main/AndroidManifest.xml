<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alex" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".NewNoteActivity"
            android:label="@string/title_activity_new_note" >
        </activity>
        <activity
            android:name="com.alex.ShowTagActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ShowNoteOfTagActivity"
            android:label="@string/title_activity_show_note_of_tag" >
        </activity>
        <activity
            android:name="com.alex.ShowNotesActivity"
            android:label="@string/title_activity_show_notes" >
        </activity>
    </application>

</manifest>
